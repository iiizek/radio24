{
	"cli": {
		"version": ">= 13.2.0",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_API_URL": "EXPO_PUBLIC_API_URL",
				"EXPO_PUBLIC_ADMIN_URL": "EXPO_PUBLIC_ADMIN_URL"
			},
			"channel": "development"
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"EXPO_PUBLIC_API_URL": "EXPO_PUBLIC_API_URL",
				"EXPO_PUBLIC_ADMIN_URL": "EXPO_PUBLIC_ADMIN_URL"
			},
			"channel": "preview"
		},
		"production": {
			"autoIncrement": true,
			"env": {
				"EXPO_PUBLIC_API_URL": "EXPO_PUBLIC_API_URL",
				"EXPO_PUBLIC_ADMIN_URL": "EXPO_PUBLIC_ADMIN_URL"
			},
			"channel": "production"
		}
	},
	"submit": {
		"production": {}
	}
}
